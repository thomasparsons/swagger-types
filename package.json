{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "generate-types-graph": "apollo codegen:generate --schema schema.json --target typescript generated",
    "generate-types-swagger": "node ./generate",
    "graph": "node graph/index.ts",
    "swagger-graph": "node graph/swagger-2.ts",
    "start": "expo start",
    "update-schema": "apollo schema:download --endpoint=http://localhost:4000/graphql schema.json",

    "create-gen": "rm ./graph/generated.ts && touch ./graph/generated.ts",
    "swagger-to-graphql": "yarn create-gen && echo const v = \\` >> ./graph/generated.ts && swagger-to-graphql --swagger=swagger.json >> ./graph/generated.ts && echo \\` >> ./graph/generated.ts && echo \n >> ./graph/generated.ts && echo module.exports = v >> ./graph/generated.ts",

    "set-up": "yarn swagger-to-graphql && yarn swagger-graph",
    "complete": "yarn update-schema && yarn generate-types-graph"
  },
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "apollo-client": "^2.5.1",
    "apollo-server": "^2.5.0",
    "apollo-server-express": "^2.5.0",
    "body-parser": "^1.18.3",
    "expo": "^32.0.0",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "fs": "0.0.1-security",
    "graphql": "^14.2.1",
    "graphql-liftoff": "git+https://github.com/target/graphql-liftoff.git",
    "graphql-tag": "^2.10.1",
    "path": "^0.12.7",
    "react": "16.8.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-picker-select": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@types/expo": "^32.0.9",
    "@types/react": "^16.8.8",
    "@types/react-native": "^0.57.41",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-expo": "^5.0.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-typescript": "1.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint-plugin-typescript": "^0.14.0",
    "graphql-tools": "^4.0.4",
    "react-apollo": "^2.5.4",
    "swagger-to-graphql": "^1.6.0",
    "swagger-ts-generator": "^1.1.56",
    "tslint": "^5.14.0"
  },
  "private": false
}
